    emotion: 0
    keyValue: 
  - sentances:
    - "Ch\xFAng ta \u0111\xE3 b\u1ECB ph\xE1 ho\u1EA1i "
    - "Xe c\u1EE7a ch\xFAng ta \u0111\u1EC1u b\u1ECB h\u1ECFng do ai \u0111\xF3 c\u1ED1
